<?xml version="1.0" encoding="UTF-8"?>
<suite name="Performance:Upload 10000 pictures">
    <parameter name="LOCAL_CONFIG_FILE" value="TAConfiguration.conf"/>
    <test verbose="2" name="Upload pictures">
        <classes>
            <class name="com.nari.jydw.jytest.performance.uploadOneThousandPictures"/>
            <class name="com.nari.jydw.jytest.performance.multiThreadsUploadPictures"/>
        </classes>
    </test>
    <listeners>
        <listener class-name="com.nari.jydw.jytest.common.ExtentTestNGIReporterListener"></listener>
    </listeners>
</suite>